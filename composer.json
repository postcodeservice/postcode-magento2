{
  "name": "tig/postcode-magento2",
  "description": "TIG Magento 2 Postcode extension",
  "require": {
    "php": "~7.0|~7.1|~7.2|~7.3|~7.4",
    "magento/module-checkout": ">=100.2.6,<=100.2.11|>=100.3.0,<=100.3.5-p2|100.4.0",
    "magento/module-quote": ">=101.0.5,<=101.0.11|>=101.1.0,<=101.1.5-p2|101.2.0"
  },
  "type": "magento2-module",
  "version": "1.2.7",
  "license": "CC-BY-NC-ND-3.0",
  "authors": [
    {
      "name": "TIG",
      "email": "support@postcodeservice.nl",
      "homepage": "https://postcodeservice.nl/"
    }
  ],
  "support": {
    "email": "support@tig.nl",
    "issues": "https://portal.tig.nl"
  },
  "homepage": "https://postcodeservice.nl/",
  "minimum-stability": "stable",
  "autoload": {
    "files": [
      "registration.php"
    ],
    "psr-4": {
      "TIG\\Postcode\\": ""
    }
  },
  "require-dev": {
    "object-calisthenics/phpcs-calisthenics-rules": "^2.0",
    "magento/marketplace-eqp": "^1.0",
    "satooshi/php-coveralls": "^1.0"
